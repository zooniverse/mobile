{
  "name": "ZooniverseMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "sed -i -- 's/class Icon extends PureComponent {/class Icon extends PureComponent {constructor(props) { super(props) }/g' node_modules/react-native-vector-icons/lib/create-icon-set.js && patch-package && sed -i '' 's/ArrayList<HashMap<String, String>>\\[\\] packages = this.reactNativeModules/ArrayList<HashMap<String, String>> packages = this.reactNativeModules/g' node_modules/@react-native-community/cli-platform-android/native_modules.gradle",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android --appIdSuffix 'debug'",
    "ios": "react-native run-ios",
    "test": "NODE_ENV=staging jest --runInBand",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native-extended-stylesheet|react-native|react-native-webview|react-navigation|react-native-vector-icons|react-native-loading-spinner-overlay)/)"
    ],
    "modulePathIgnorePatterns": [
      "node_modules/react-native-deck-swiper/node_modules/react-native"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+.png$": "RelativeImageStub"
    }
  },
  "dependencies": {
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/netinfo": "^4.6.1",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@sentry/react-native": "3.2.10",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "color": "^3.1.0",
    "crypto-js": "^3.1.9-1",
    "envinfo": "^6.0.1",
    "jetifier": "^1.6.4",
    "jsc-android": "^241213.1.0",
    "json5": "^2.1.0",
    "markdown-it": "^8.3.1",
    "metro": "^0.51.1",
    "panoptes-client": "^3.0.0-rc.0",
    "patch-package": "^6.2.0",
    "prop-types": "^15.8.1",
    "ramda": "^0.25.0",
    "react": "16.8.6",
    "react-native": "0.62.3",
    "react-native-browser-builtins": "^2.0.3",
    "react-native-checkbox-field": "^2.0.2",
    "react-native-deck-swiper": "github:zooniverse/react-native-deck-swiper",
    "react-native-device-info": "2.1.2",
    "react-native-drawer": "^2.3.0",
    "react-native-extended-stylesheet": "^0.8.1",
    "react-native-image-pan-zoom": "^2.0.16",
    "react-native-image-size": "^1.1.3",
    "react-native-loading-spinner-overlay": "^0.5.2",
    "react-native-router-flux": "4.0.6",
    "react-native-simple-markdown": "*1.1.0",
    "react-native-simple-store": "^1.1.0",
    "react-native-splash-screen": "^3.0.6",
    "react-native-svg": "9.13.3",
    "react-native-swiper": "^1.6.0-nightly.5",
    "react-native-vector-icons": "^7.0.0",
    "react-native-video": "^5.2.0",
    "react-native-webview": "^7.5.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.1.0",
    "rn-fetch-blob": "^0.11.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.15.4",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-react-native": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "jest": "^24.9.0",
    "jest-cli": "23.6.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-test-renderer": "16.8.6"
  }
}
